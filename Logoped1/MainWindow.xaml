<Window x:Class="Logoped1.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Logoped1"
        mc:Ignorable="d"
        Height="700" Width="1400" Icon="/логотип.jpg" WindowStartupLocation="CenterScreen">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="70"/>
            <RowDefinition Height="*"/>
            <RowDefinition Height="60"/>
        </Grid.RowDefinitions>
        <TextBlock Text="Главный экран" Style="{StaticResource TextBlock1}" Width="240" VerticalAlignment="Center"/>
        <Button Name="BtnBack" Content="Вернуться" Click="BtnBack_Click" Style="{StaticResource MainWindowBtn}" HorizontalAlignment="Right"/>

        <Frame Name="MainFrame" Grid.Row="1" NavigationUIVisibility="Hidden" ContentRendered="MainFrame_ContentRendered"/>
        <Image Source="/Resources/логотип.jpg" Panel.ZIndex="-1" Grid.Row="1" HorizontalAlignment="Left"/>
    </Grid>
</Window>
